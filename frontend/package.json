{
  "name": "boiler-stack-frontend",
  "private": true,
  "version": "1.0.1",
  "description": "the frontend for the boilerstack app",
  "main": "index.js",
  "scripts": {
    "clean": "mkdir -p dist && rm -rf dist/*",
    "preassets": "npm run clean",
    "assets": "cp -r www/* dist",
    "prewatch": "npm run assets",
    "watch": "WEBPACK_ARGS=\"--watch --progress\" npm run webpack",
    "prewebpackall": "npm run clean && npm run assets",
    "webpack": "webpack $WEBPACK_ARGS",
    "webpack:json": "WEBPACK_ARGS=--json npm run webpack",
    "webpackall2": "APP=app npm run webpack && APP=admin npm run webpack",
    "webpackall": "APP=app npm run webpack",
    "build": "NODE_ENV=development npm run webpackall",
    "release": "WEBPACK_ARGS=-p NODE_ENV=production npm run webpackall",
    "build:stats": "webpack -p --json | webpack-bundle-size-analyzer",
    "build:test": "rollup src/app/index.js --output bundle.js"
  },
  "dependencies": {
    "immutability-helper": "^2.0.0",
    "babel-polyfill": "^6.16.0",
    "boiler-frontend": "file:../../boiler-frontend",
    "kettle-ui": "^3.7.0",
    "material-ui": "^0.16.4",
    "passport-slim-ui": "file:../../passport-slim-ui",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.6.1",
    "rollup": "^0.36.4",
    "webpack": "^2.1.0-beta.26",
    "webpack-bundle-size-analyzer": "^2.2.0"
  }
}
