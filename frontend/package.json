{
  "name": "boiler-stack-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "the frontend for the boilerstack app",
  "main": "index.js",
  "scripts": {
    "clean": "mkdir -p dist && rm -rf dist/*",
    "assets": "cp www/* dist",
    "watch": "WEBPACK_ARGS=--watch npm run webpack",
    "prewebpack": "npm run clean && npm run assets",
    "webpack": "webpack $WEBPACK_ARGS",
    "build": "NODE_ENV=development npm run webpack",
    "release": "NODE_ENV=production npm run webpack"
  },
  "dependencies": {
    "boiler-frontend": "^2.2.3",
    "deep-get-set": "^1.0.0",
    "digger-folder-ui-db": "^1.0.0",
    "folder-ui": "^3.0.0",
    "kettle-ui": "^3.0.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chokidar": "^1.6.0",
    "webpack": "^1.12.12"
  }
}
